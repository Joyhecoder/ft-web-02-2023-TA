<link href="https://fonts.googleapis.com/css2?family=Carlito:ital,wght@0,400;0,700;1,400&display=swap" rel="stylesheet">

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

    <title>Document</title>
  </head>
  <body>
    <div class="container-fluid">

        <!-- nav section -->
        <div className="nav-container">
            <div class="nav-taylor-swift">
                <h2>TAYLOR SWIFT</h2>
            </div>
            <nav class="navbar navbar-expand-sm ">

                <!-- Links -->
                <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" href="#">HOME</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">MUSIC</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="#">SHOWS</a>
                </li>
                </ul>
            
            </nav>
        </div>

        <!-- end of nav section -->

        <!-- picture section -->
        <div class="picture-section">
            <!-- carsoul section -->
            <div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
                <div class="carousel-inner">
                <div class="carousel-item active">
                    <img class="d-block w-100" src="https://w0.peakpx.com/wallpaper/441/792/HD-wallpaper-taylor-swift-sexy-charm-2020-high-quality.jpg" alt="First slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://w0.peakpx.com/wallpaper/132/130/HD-wallpaper-taylor-swift-actress-2016-beauty-high-quality.jpg" alt="Second slide">
                </div>
                <div class="carousel-item">
                    <img class="d-block w-100" src="https://cdn.wallpapersafari.com/2/79/qAQjOl.jpg" alt="Third slide">
                </div>
                </div>
            </div>

            <!-- word div section -->
            <div id="word">
                <h1>New Album!</h1>
                <h5>"MIDNIGHTs" Streaming NOW!</h5>
                <button type="submit" class="btn btnStyle" onclick="location.href='https://open.spotify.com/album/151w1FgRZfnKZA9FEcg9Z3'" formtarget="_blank">PLAY</button>

            </div>
        </div>
        <!-- end of picture section -->

        <!-- intro and form section -->
        <div class="intro-section">
            <div class="word" width="65vw">
                <h1>Welcome to Taylor Swift Music!</h1>
                <p>Thanks for stopping by the fan website for Taylor Swift. Be sure to sign up for the mailing list to be among the first to know about new shows, events, chat with other Swiftie and more! </p>
            </div>
            <div class="form" width="35vw">
                <h5>Join our mailing list for the latest news</h5>
                <input type="text" placeholder="Your email">
                <button type="button" class="btn signUpbtn">SIGN UP</button>

            </div>
        </div>
        <!-- end of intro and form section -->
    </div>  

  </body>
</html>
